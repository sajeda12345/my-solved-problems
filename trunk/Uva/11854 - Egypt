#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
long long int a[3];

while(cin>>a[0]>>a[1]>>a[2]&&(a[0]!=0&&a[1]!=0&&a[2]!=0))
{
sort(a,a+3);
  
  if( a[2]*a[2] = ( a[1]*a[1] ) + ( a[0]*a[0] ) )
    {
      cout<<"right"<<endl;
    }
  else
    {
      cout<<"wrong"<<endl;
    }
 }   
    return 0;
    
}