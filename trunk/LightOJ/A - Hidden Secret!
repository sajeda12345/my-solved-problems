#define pb push_back
#define mp make_pair
#define si(x) scanf("%d",&x)
#define st(x,y,z) scanf("%d%d%d",&x,&y,&z)
#define src(x,y) scanf("%d%d",&x,&y)
#define pi(x) printf("%d",x)
#define sc(c) scanf("%s",&c)
#define pc(c) printf("%s",c)
#define int64 long long int
#define maxn 110
#define fop freopen("acm.txt", "r", stdin)
#define kas printf("Case %d: ",x)
#define nl printf("\n")

using namespace std;

int main()
{
   fop;
    int t,kase=0;
    si(t);
    while(t--)
    {
        kase++;
        char s1[101], s2[101];
        sc(s1);
        sc(s2);
        int len1 = strlen(s1), len2 = strlen(s2);
        if(len1!=len2)
        {
            kas(kase);
            printf("No\n");

        }
        else
        {
            kas(kase);
            int sum1=0, sum2=0;
            for(int i=0; i<len1; i++)
            {
                sum1 += (int) toupper(s1[i]);
                sum2 += (int) toupper(s2[i]);
            }

            if(sum1==sum2)
            {
                printf("Yes\n");
            }
            else
            {
                printf("No\n");
            }

        }




    }

    return 0;
}
